QPushButton {
    background-color: #4CAF50; /* Green */
    border: none;
    color: white;
    padding: 8px 16px;
    text-align: center;
    text-decoration: none;
    font-size: 14px;
    margin: 4px 2px;
    border-radius: 4px;
}
QPushButton:hover {
    background-color: #45a049;
}
QLineEdit {
    border: 1px solid #ccc;
    padding: 5px;
    border-radius: 3px;
}